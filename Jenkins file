pipeline {
  agent any

    stages {
        stage('transfer ZIP from local to remote server') {

            steps {

              script {

                  bat 'scp "C:/Users/ASUS/Downloads/mysql-connector-java-8.0.17.rar" ubuntu@13.201.64.199:/home/ubuntu/'

}
}
}
}

post {

success {

echo 'file copy successfull'
}

failure {

echo 'file copy failed'
}
}
}
